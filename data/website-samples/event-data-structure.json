    "requiredFields": [
      "name",
      "startDate", 
      "endDate",
      "day",
      "time", 
      "bar",
      "city"
    ],
    "optionalFields": [
      "cover",
      "tea", 
      "coordinates",
      "links",
      "eventType",
      "recurring",
      "source",
      "address"
    ]
  },
