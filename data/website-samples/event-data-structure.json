  "bearKeywords": {
    "description": "Keywords for identifying bear events",
    "primary": [
      "bear", "bears", "cub", "cubs", "otter", "otters",
      "daddy", "daddies", "woof", "grr", "furry", "hairy"
    ],
    "events": [
